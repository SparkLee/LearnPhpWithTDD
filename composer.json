{
    "name": "sparklee/php-tdd-scaffold",
    "type": "project",
    "description": "PHP TDD（测试驱动开发）脚手架",
    "keywords": [
        "TDD",
        "PHPUnit",
        "Scaffold"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Spark Lee",
            "email": "liweijsj@163.com"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5||^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
